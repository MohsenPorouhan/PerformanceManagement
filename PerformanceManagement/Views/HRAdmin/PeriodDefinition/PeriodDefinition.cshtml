        onSelect: function (dateText, inst) {
            $('.datepicker12to').datepicker('option', 'minDate',
                new JalaliDate(inst['selectedYear'], inst['selectedMonth'], inst['selectedDay']));
        },
        showWeek: true,
        showButtonPanel: true,
        changeMonth: true,
        changeYear: true,
        dateFormat: "yy/mm/dd"
    });
    $('.datepicker12to').datepicker({
        showWeek: true,
        showButtonPanel: true,
        changeMonth: true,
