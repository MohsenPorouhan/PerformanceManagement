                                toastr.options.positionClass = "toast-top-center";
                                toastr.success(result);
                            }
                        })
                        if (message != "") {
                            toastr.options.timeOut = "15000";
                            toastr.options.positionClass = "toast-top-center";
                            toastr.info(message);
                        }
                        $("#periodDefinitionFrm")[0].reset();
