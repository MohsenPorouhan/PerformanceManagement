@{
    Layout = null;
}
@model List<Truth>
<table class="table table-striped table-bordered table-hover" id="sample_dt">
    <thead>
        <tr>
            <th id="th1" hidden>
                behaviouralCompetencyId
            </th>
            <th id="th1" hidden>
                truthId
            </th>
            <th id="th2">
                نام مصداق
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td hidden>@item.BehaviouralCompetencyId</td>
                <td hidden>@item.TruthId</td>
                <td>@item.Title</td>
            </tr>
        }

    </tbody>
</table>
