@{
    Layout = null;
}
@model List<ScoreView>
@if (ViewBag.type == 1)
{
    <table class="table table-striped table-bordered table-hover" id="sample_dt">
        <thead>
            <tr>

                <th id="th1">
                    نام شایستگی
                </th>
                <th id="th4">
                    نمره سایرارزیاب
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Title</td>
                    <td>@item.participantScore</td>
                </tr>
            }

        </tbody>
    </table>
}
